{
    "name": "metalinked/laravel-defender",
    "description": "Modular security package for Laravel",
    "type": "library",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Metalinked\\LaravelDefender\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Metalinked\\LaravelDefender\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
