{
    "name": "metalinked/laravel-defender",
    "description": "Modular security package for Laravel",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.9",
        "laravel/slack-notification-channel": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Metalinked\\LaravelDefender\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Metalinked\\LaravelDefender\\DefenderServiceProvider"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Metalinked\\LaravelDefender\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
